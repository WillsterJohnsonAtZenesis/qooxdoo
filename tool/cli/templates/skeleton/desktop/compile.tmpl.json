{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": ${locales},
  "environment": { 
      "qx.icontheme": "${icon_theme}"
  },     
  "applications": [
    {
      "class": "${namespace}.Application",
      "theme": "${namespace}.theme.Theme",
      "name": "${namespace}",
      "writeIndexHtmlToRoot": true
    }
  ],

  "libraries": [
    "${qxpath}",
    "."
  ]
}