{
	"targets": [
		{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],

	"defaultTarget": "source",

	"locales": [ "en" ],

	"applications": [
		{
			"class": "testapp.Application",
			"theme": "qx.theme.Indigo",
			"name": "testapp",
			"environment": {
                "test.overridden1": true,
                "test.overridden2": false
			}
		}
	],

	"environment": {
		"test.isFalse": false,
		"test.isTrue": true,
		"test.someValue": "some",
        "test.overridden1": false,
        "test.overridden2": true
	},

    "bundle": {
       "include": [ "qx.*" ],
       "exclude": [ "qx.util.*" ]
    },

	"parts": {
		"boot": {
			"include": [ "testapp.*" ]
		},
        "pluginFramework": {
            "include": [ "testapp.pdk.*" ]
        },
        "utils": {
            "include": [ "testapp.utils.*" ]
        },
        "pluginOne": {
            "include": [ "testapp.plugins.PluginOne", "testapp.plugins.One*" ]
        },
        "pluginTwo": {
            "include": [ "testapp.plugins.PluginTwo", "testapp.plugins.Two*" ]
        }
	}
}
