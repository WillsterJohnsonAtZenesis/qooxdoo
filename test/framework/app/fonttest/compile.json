{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "fonts": {
        "local": false
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],
  "environment": {
    "qx.icontheme": "Tango"
  },
  "applications": [
    {
      "class": "fonttest.Application",
      "theme": "qx.theme.TangibleLight",
      "name": "fonttest",
      "bootPath": "source/boot"
    }
  ]
}
